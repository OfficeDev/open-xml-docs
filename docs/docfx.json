{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "ms.subservice": "open-xml",
      "ms.service": "office",
      "breadcrumb_path": "/office/open-xml/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "uhfHeaderId": "MSDocsHeader-Dev_Office",
      "ms.suite": "office",
      "ms.author": "o365devx",
      "author": "o365devx",
      "ms.topic": "conceptual",
      "description": "Use the Open XML SDK to programmatically create Office Word, Excel, and PowerPoint documents, and manipulate their content."
    },
    "fileMetadata": {
      "langs": {
        "**/*.yml": ["csharp", "vb"]
      }
    },
    "template": [
      "default",
      "modern"
    ],
    "dest": "open-xml-docs",
    "markdownEngineName": "markdig"
}
}

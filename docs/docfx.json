{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/office/open-xml/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "uhfHeaderId": "MSDocsHeader-Dev_Office",
      "ms.suite": "office",
      "ms.author": "o365devx",
      "author": "o365devx",
      "ms.topic": "conceptual",
      "ms.prod": "office",
      "ms.technology": "open-xml"
    },    
      "langs": {
      "**/*.yml": ["csharp", "vb"]
      }
    },
    "fileMetadata": {},
    "template": [],
    "dest": "open-xml-docs",
    "markdownEngineName": "markdig"
}
